    "def get_CIP(target_code, df):\n",
    "    out = pd.DataFrame()\n",
    "    out['BGTResID'] = None\n",
    "    for i in range(12):\n",
    "        temp = df[df['CIPCode'+str(i)] % target_code < 1]\n",
    "        out.merge(temp, on='BGTResID', how='outer')\n",
    "        \n",
    "    return out\n",
    "get_CIP(23, cip)"
   ]
